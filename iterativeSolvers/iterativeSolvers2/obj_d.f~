C  Differentiation of obj in forward (tangent) mode:
C   variations   of useful results: y
C   with respect to varying inputs: xold
C
      SUBROUTINE OBJ_D(xold, xoldd, y, yd)
      IMPLICIT NONE
      DOUBLE PRECISION xold(4), y
      DOUBLE PRECISION xoldd(4), yd
      yd = xoldd(1) + 2*xoldd(2) + 3*xoldd(3) + 4*xoldd(4)
      y = xold(1) + 2*xold(2) + 3*xold(3) + 4*xold(4)
      END
